{
  "version": 2,
  "builds": [
    {
      "src": "mysite/asgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "mysite/asgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "mysite.settings",
    "PYTHONUNBUFFERED": "1"
  }
}
